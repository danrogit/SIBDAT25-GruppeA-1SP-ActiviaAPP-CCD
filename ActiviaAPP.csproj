<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>Img\activia-icon.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Img\activia-icon.ico" />
    <None Remove="Img\activia-logo.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Img\activia-icon.ico" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Img\activia-icon.ico" />
    <Resource Include="Img\activia-logo.png" />
  </ItemGroup>

</Project>
